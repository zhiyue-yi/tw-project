{
	"defaultSeverity": "error",
	"extends": [
		"tslint-config-prettier"
	],
	"rules": {
		"quotemark": {
			"options": ["single", "jsx-double"]
		},
		"array-type": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"no-unused-expression": {
			"serverity": "off"
		},
		"no-console": false,
		"trailing-comma": true,
		"indent": {
			"options": ["spaces", 4],
			"severity": "error"
		},
		"no-var-requires": false,
		"variable-name": false,
		"no-shadowed-variable": false
	}
}